{
  "name": "revolud-exchange",
  "version": "0.0.1",
  "private": true,
  "description": "Exchange UI screen for revolut",
  "author": {
    "name": "Ivan Egorov",
    "email": "vany.egorov@gmail.com",
    "url": "https://github.com/vany-egorov"
  },
  "license": "MIT",
  "scripts": {
    "build": "better-npm-run build:dev",
    "dev": "better-npm-run build:dev",
    "prod": "better-npm-run build:prod",
    "release": "better-npm-run build:prod",
    "server": "better-npm-run server:dev",
    "clean": "rimraf target"
  },
  "betterScripts": {
    "build:dev": {
      "command": "node ./bin/build",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development",
        "UI_ENV": "dev"
      }
    },
    "build:prod": {
      "command": "npm run clean && node ./bin/build",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production",
        "UI_ENV": "prod"
      }
    },
    "server:dev": {
      "command": "nodemon ./bin/server.js --ignore src --ignore target --ignore node_modules",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development",
        "UI_ENV": "dev"
      }
    }
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "baconjs": "^0.7.89",
    "better-npm-run": "^0.0.14",
    "classnames": "^2.2.5",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.26.2",
    "d3": "^4.5.1",
    "debug": "^2.6.0",
    "eslint": "^3.13.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "http-proxy-middleware": "^0.17.3",
    "lodash": "^4.17.4",
    "nodemon": "^1.11.0",
    "normalize.css": "^5.0.0",
    "npm": "^4.3.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.2",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.9.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "reconnectingwebsocket": "^1.0.0",
    "redbox-react": "^1.3.4",
    "redux": "^3.6.0",
    "rimraf": "^2.6.1",
    "socket.io": "^1.7.3",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-closure-compiler": "^2.1.4",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  },
  "devDependencies": {
    "request": "^2.81.0"
  }
}
